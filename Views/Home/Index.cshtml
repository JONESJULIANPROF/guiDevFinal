@{
    ViewData["Title"] = "Home Page";
    @model FianlGUI.Models.FatTrack
}

<div class="text-center">
    <h1 class="display-2">Welcome to FatTrak</h1>
    <h2 class="display-4">Your one stop health shop!</h2>
    <br />
    <br />
    <br />


    
    <form name="FoodEntry" method="post" asp-action="Index">
        <div class="form-group">
            @{
                if (Model != null)
                {
                    <h1 class="display-4">@Model.FoodCalories</h1>
                }

            }
            

            <label asp-for="FoodCalories">Food:</label>
            <input class="form-control" asp-for="FoodCalories" id="FoodCalories" placeholder="FoodName" />


        </div>
        <button class="btn btn-primary" type="submit">Submit Food</button>
    </form>



    <!--<p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>-->
</div>
