@model FianlGUI.Models.FatTrack
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
   
}
<h1>Your Info</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Starting Weight</th>
            <th>Current Weight</th>
            <th>Desired Weight</th>
            <th>Height</th>
            <th>Gender</th>
            <th>Activity Level</th>
            <th>Birthday</th>
            <th>Age</th>
            <th>BMR</th>
            <th>Calories Needed</th>
        </tr>

    </thead>

    <tbody>
        @{
            if (Model != null)
            {
                <tr>
                    <td>@Model.FirstN</td>
                    <td>@Model.Email</td>
                    <td>@Model.StartingW</td>

                    <td>@Model.DesiredW</td>
                    <td>@Model.HeightInches</td>
                    <td>@Model.Gender</td>
                    <td>@Model.ActivityLevel</td>
                    <td>@Model.Birthday</td>
                    <td>@Model.Age</td>

                </tr>

            }



        }


    </tbody>

</table>

